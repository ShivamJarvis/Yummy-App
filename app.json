{
  "expo": {
    "name": "yummy",
    "slug": "yummy",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/yummy-icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/Yummy-1-white.png",
      "resizeMode": "contain",
      "backgroundColor": "#f78783"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/Yummy-1-white.png",
        "backgroundColor": "#f78783"
      }
    },
    "web": {
      "favicon": "./assets/yummy-icon.png"
    }
  }
}
